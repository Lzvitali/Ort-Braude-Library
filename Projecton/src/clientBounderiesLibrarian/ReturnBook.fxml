<AnchorPane prefHeight="186.0" prefWidth="428.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <JFXTextField fx:id="CopyIdTextFielf" labelFloat="true" layoutX="120.0" layoutY="90.0" prefHeight="25.0" prefWidth="138.0" promptText="Enter Copy ID">
