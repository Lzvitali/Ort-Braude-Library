            <Font size="14.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="OkBtn" buttonType="RAISED" layoutX="294.0" layoutY="92.0" onAction="#OkBtnClicked" prefHeight="14.0" prefWidth="70.0" text="Ok">
         <font>
            <Font size="14.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="CancelBtn" layoutX="14.0" layoutY="148.0" onAction="#CancelBtnClicked" prefHeight="25.0" prefWidth="76.0" text="Cancel">
         <font>
            <Font size="14.0" />
         </font>
      </JFXButton>
      <ImageView fitHeight="45.0" fitWidth="297.0" layoutX="72.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../icons/titles-librarian/returnBook.gif" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
